//Implicit Return
func findRemainder(dividend:Int, divisor:Int) -> Int {
 dividend % divisor
}
print(findRemainder(dividend:10, divisor:4))

//It is the same thing as the code block below:
func findRemainder(dividend:Int, divisor:Int) -> Int {
 return dividend % divisor
}
print(findRemainder(dividend:10, divisor:4))
